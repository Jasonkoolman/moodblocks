{
  "name": "@moodblocks/blockchain",
  "version": "0.1.0",
  "description": "Moodblocks contracts",
  "main": "index.ts",
  "scripts": {
    "build": "yarn build:sol && yarn build:ts",
    "build:ts": "tsc -p .",
    "build:sol": "npx hardhat compile",
    "hardhat": "npx hardhat",
    "task:accounts": "npx hardhat accounts",
    "task:deploy": "npx hardhat run tasks/sample-script.js",
    "task:mint-noun": "npx hardhat mint-noun",
    "task:populate-descriptor": "npx hardhat populate-descriptor",
    "task:run-local": "npx hardhat run-local",
    "dev": "npx hardhat node",
    "dev:deploy": "npx hardhat deploy --network localhost",
    "dev:console": "npx hardhat console --network localhost",
    "test": "npx hardhat test",
    "coverage": "npx hardhat coverage"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.3.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.6",
    "hardhat": "^2.6.2",
    "hardhat-abi-exporter": "^2.3.0",
    "mocha": "^9.1.1",
    "ts-node": "^10.2.1",
    "typechain": "^5.1.2"
  }
}
